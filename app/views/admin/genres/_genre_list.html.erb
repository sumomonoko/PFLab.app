<table class="table table-borderless">
  <h5 class="text-success">管理者ログイン中</h5>
</table>
<div class="genre_search">
  <table class="table table-bordered">
    <tr>
      <th class="text-center">ジャンル検索</th>
    </tr>
    <%= form_with url: admin_genre_search_path, method: :get do |f| %>
      <% genres.each do |genre| %>
        <tr>
          <td><%= link_to genre.name, admin_genre_search_path(genre_id: genre.id), method: :get %></td>
        </tr>
      <% end %>
    <% end %>
  </table>
</div>